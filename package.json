{
  "name": "nestjs-starter-api",
  "version": "0.0.1",
  "description": "NestJS Prisma Client Starter Project",
  "author": "Luis Miguel Alvarado",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "prisma",
    "typescript",
    "passport"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ecreeth/nestjs-starter.git"
  },
  "bugs": {
    "url": "https://github.com/ecreeth/nestjs-starter/issues"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:status": "npx prisma migrate status",
    "migrate:resolve": "npx prisma migrate resolve",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "prisma:push": "npx prisma db push",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run prisma:generate && npm run migrate:deploy && npm run seed",
    "seed": "prisma db seed",
    "postinstall": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "10.1.0",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.0",
    "@nestjs/event-emitter": "^2.0.1",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.1.0",
    "@nestjs/schedule": "^3.0.2",
    "@nestjs/swagger": "7.1.2",
    "@nestjs/throttler": "^4.2.1",
    "@prisma/client": "^5.7.1",
    "bcrypt": "5.1.0",
    "bull": "^4.11.3",
    "cache-manager": "^5.2.3",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "google-auth-library": "^9.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "nestjs-prisma": "0.22.0",
    "nodemailer": "^6.9.5",
    "otplib": "^12.0.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "^1.0.0",
    "qrcode": "^1.5.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@nestjs/cli": "10.1.10",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.1.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.70",
    "@types/bcrypt": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.4.0",
    "@types/express": "4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.10.6",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/qrcode": "^1.5.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "chance": "1.1.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.6.1",
    "prettier": "2.8.8",
    "prisma": "^5.7.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
